{
    "name": "yapcdi/symfony-ctrl-resolver",
    "type": "library",
    "description": "Controller resolver with di management container",
    "keywords": ["di", "dependency", "injection", "cdi"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Schlegel",
            "email": "alexander@yapcdi.com"
        }
    ],
    "require": {
        "php": ">=5.3",
        "symfony/http-kernel": "^2.6.6",
        "yapcdi/yapcdi": "^1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*"
    },
    "autoload": {
        "psr-4": {
            "yapcdi\\controller\\resolver\\symfony\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "yapcdi\\controller\\resolver\\symfony\\tests\\": "tests/"
        }
    }
}
